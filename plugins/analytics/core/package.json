{
  "name": "@snap-agent/analytics",
  "version": "0.1.0",
  "description": "Comprehensive analytics plugin for SnapAgent SDK - Performance, RAG, Cost, Conversation, and Error metrics",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean --external mongodb",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch --external mongodb",
    "lint": "eslint src/"
  },
  "keywords": [
    "snap-agent",
    "analytics",
    "metrics",
    "observability",
    "performance",
    "monitoring",
    "ai",
    "llm",
    "rag",
    "plugin"
  ],
  "author": "ViloTech",
  "license": "MIT",
  "peerDependencies": {
    "@snap-agent/core": "^0.1.0",
    "mongodb": "^6.0.0 || ^7.0.0"
  },
  "peerDependenciesMeta": {
    "mongodb": {
      "optional": true
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.0.0",
    "mongodb": "^7.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vilotech/snap-agent",
    "directory": "plugins/analytics/core"
  }
}

