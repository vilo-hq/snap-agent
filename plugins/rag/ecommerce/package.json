{
  "name": "@snap-agent/rag-ecommerce",
  "version": "0.1.2",
  "description": "E-commerce RAG plugin for SnapAgent SDK - Product search with vector embeddings",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "rag",
    "ecommerce",
    "vector-search",
    "product-recommendations",
    "ai"
  ],
  "author": "ViloTech",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@snap-agent/core": "^0.1.0",
    "mongodb": "^6.0.0"
  },
  "dependencies": {
    "openai": "^5.0.1"
  },
  "devDependencies": {
    "@types/node": "^24.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.8.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vilo-hq/snap-agent.git",
    "directory": "plugins/rag/ecommerce"
  },
  "homepage": "https://github.com/vilo-hq/snap-agent/tree/main/plugins/rag/ecommerce",
  "bugs": {
    "url": "https://github.com/vilo-hq/snap-agent/issues"
  }
}
